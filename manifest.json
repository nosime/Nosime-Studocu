{
  "manifest_version": 3,
 
  "name": "Nosime-Studocu",
  "version": "1.0.2",
  "description": "Banner remover, filter pass & document downloader - By Võ Quốc Sơn",
  "icons": {
    "16": "images/nosime16.png",
    "48": "images/nosime48.png",
    "128": "images/nosime128.png"
  },
 
  "content_scripts": [{
    "js": ["version-check.js", "remove-banner.js", "remove-blur.js", "doc-viewer.js"],
    "css": ["style.css"],
    "matches": ["https://www.studocu.com/*", "https://www.studeersnel.nl/*"]
  }]
}
